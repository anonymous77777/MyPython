<div class=3D"highlight-html+django"><div class=3D"highlight"><pre><span></=
span>import sqlite3
from bottle import route, run, debug, template, request, static_file, error

# only needed when you run Bottle on mod_wsgi
from bottle import default_app

@route('/todo')
def todo_list():

    conn =3D sqlite3.connect('todo.db')
    c =3D conn.cursor()
    c.execute("SELECT id, task FROM todo WHERE status LIKE '1';")
    result =3D c.fetchall()
    c.close()

    output =3D template('make_table', rows=3Dresult)
    return output

@route('/new', method=3D'GET')
def new_item():

    if request.GET.get('save','').strip():

        new =3D request.GET.get('task', '').strip()
        conn =3D sqlite3.connect('todo.db')
        c =3D conn.cursor()

        c.execute("INSERT INTO todo (task,status) VALUES (?,?)", (new,1))
        new_id =3D c.lastrowid

        conn.commit()
        c.close()

        return '&lt;p&gt;The new task was inserted into the database, the I=
D is %s&lt;/p&gt;' % new_id

    else:
        return template('new_task.tpl')

@route('/edit/&lt;no:int&gt;', method=3D'GET')
def edit_item(no):

    if request.GET.get('save','').strip():
        edit =3D request.GET.get('task','').strip()
        status =3D request.GET.get('status','').strip()

        if status =3D=3D 'open':
            status =3D 1
        else:
            status =3D 0

        conn =3D sqlite3.connect('todo.db')
        c =3D conn.cursor()
        c.execute("UPDATE todo SET task =3D ?, status =3D ? WHERE id LIKE ?=
", (edit,status,no))
        conn.commit()

        return '&lt;p&gt;The item number %s was successfully updated&lt;/p&=
gt;' %no

    else:
        conn =3D sqlite3.connect('todo.db')
        c =3D conn.cursor()
        c.execute("SELECT task FROM todo WHERE id LIKE ?", (str(no)))
        cur_data =3D c.fetchone()

        return template('edit_task', old =3D cur_data, no =3D no)

@route('/item&lt;item:re:[0-9]+&gt;')
def show_item(item):

        conn =3D sqlite3.connect('todo.db')
        c =3D conn.cursor()
        c.execute("SELECT task FROM todo WHERE id LIKE ?", (item))
        result =3D c.fetchall()
        c.close()

        if not result:
            return 'This item number does not exist!'
        else:
            return 'Task: %s' %result[0]

@route('/help')
def help():

    static_file('help.html', root=3D'.')

@route('/json&lt;json:re:[0-9]+&gt;')
def show_json(json):

    conn =3D sqlite3.connect('todo.db')
    c =3D conn.cursor()
    c.execute("SELECT task FROM todo WHERE id LIKE ?", (json))
    result =3D c.fetchall()
    c.close()

    if not result:
        return {'task':'This item number does not exist!'}
    else:
        return {'Task': result[0]}


@error(403)
def mistake403(code):
    return 'There is a mistake in your url!'

@error(404)
def mistake404(code):
    return 'Sorry, this page does not exist!'


debug(True)
run(reloader=3DTrue)
